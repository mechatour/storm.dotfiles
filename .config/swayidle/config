timeout 1800 '~/.config/swaylock/screenoff.sh' resume '~/.config/swaylock/screenon.sh'
timeout 1805 '~/.config/swaylock/lock.sh'
timeout 30 '~/.config/swaylock/lockedscreenoff.sh' resume '~/.config/swaylock/lockedscreenon.sh'
before-sleep '~/.config/swaylock/lock.sh'
lock '~/.config/swaylock/lock.sh'
