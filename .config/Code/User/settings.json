{
    "[python]": {
        "editor.formatOnType": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "git.enableSmartCommit": true,
    "frontMatter.data.folders": [],
    "git.openRepositoryInParentFolders": "never",
    "frontMatter.content.fmHighlight": false,
    "frontMatter.site.baseURL": "https://linux.zanegodden.com",
    "frontMatter.templates.enabled": true,
    "cSpell.ignoreWords": [
        "behaviour"
    ],
    "cSpell.userWords": [
        "Customisation"
    ],
    "cmake.configureOnOpen": false,
    "workbench.colorTheme": "Storm",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "git.confirmSync": false,
    "debug.onTaskErrors": "abort",
    "cmake.showOptionsMovedNotification": false,
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.debugShortcut": true,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "cSpell.enabled": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": []
    },
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#644040",
        "editorBracketHighlight.foreground2": "#406440",
        "editorBracketHighlight.foreground3": "#646440",
        "editorBracketHighlight.foreground4": "#404064",
        "editorBracketHighlight.foreground5": "#644064",
        "editorBracketHighlight.foreground6": "#406464",
        "editorBracketHighlight.unexpectedBracket.foreground": "#cfcfd4"
    },
    "editor.inlayHints.enabled": "offUnlessPressed",
    "go.toolsManagement.autoUpdate": true,
    "lldb.suppressUpdateNotifications": true,
    "remote.enableRemoteServer": false,
    "editor.fontFamily": "Hack Nerd Font",
    "editor.fontSize": 14,
    "update.mode": "none",
    "editor.tabSize": 3,
   "editor.detectIndentation": false,
   "zig.zls.enabled": "on",
   "extensions.experimental.affinity": {
      "asvetliakov.vscode-neovim": 1
   },
   "makefile.configureOnOpen": true
}